body{
    background: white;
    font-family: PingFangSC-Medium;
}
.header-nav1{
    background-color: rgb(254,196,52)!important;
}
.header-nav1 p{
    margin: 0px;
    font-family: PingFangSC-Medium;
    font-size: 18px;
    color: #ffffff;
    text-align: center;
}
.header-nav1 img{
    margin-left: -2px;
    margin-top: -5px
}
.am-header .am-header-nav img {
    /*height: 16px;*/
    /*width: auto;*/
    width: 15px;
    height: 26px;
    vertical-align: middle;
}
.header-nav1 span {
    color: white;
    /*margin: 0px;*/
    font-size: 18px;
}
li.am-panel>p {
    background-color: white;
    height: 42px;
    line-height: 42px;
    vertical-align: middle;
    margin: 0px;
    font-family:PingFangSC-Regular;
    font-size:16px;
    color:#4a4a4a;
    /*line-height:27px;*/
    text-align:left;
}

select{
    float: right;
    height: inherit;
    width: 170px;
    border: 1px solid transparent;
}
input{
    width: 170px;
    height: 100%;
    float: right;
    outline: none;
    border: none;
    color: black;
    padding-left: 0px;
    display:inline;
    padding-bottom: 0px;

}

button{
    opacity:0.69;
    background:#ffba0a;
    border-radius:39px;
    width:174px;
    height:44px;
    border: none;
    font-size: 20px;
}

.content{
    width: 100%;
    height: auto;
    background: white;
    padding: 25px 10px;
}
.content-zzxx{
    text-align: center;
    color: black;
    background-color: rgb(242,242,242)!important;
    font-weight: 200;
}

.baocun{
    height: auto;
    width: 100%;
    padding-top: 30px;
    text-align: center;
    /*padding: 25% 25%;*/
    color: white;
}
#message{
    text-align: center;
    height: 30px;
    width: 100%;
    color: black
}

#notice{
    position: fixed;
    top: 80%;
    background-color: #ff8625;
    width: 50%;
    left: 25%;
    height: 3rem;
    border-radius: 1.5rem;
    box-shadow: black;
    line-height: 3rem;
    vertical-align: middle;
    text-align: center;
    box-shadow: 0px 2px 10px #888888;
    display: none;
    z-index: 999;
}

