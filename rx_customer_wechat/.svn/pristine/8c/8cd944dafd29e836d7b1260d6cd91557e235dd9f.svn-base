public class TransferinfoReq {
    /**
     * sign
     */
    private String  sign;
    /**
     *商户订单号
     */
    private String partner_trade_no;
    /**
     * mch_id
     */
    private String mch_id;
    /**
     * Appid
     */
    private String appid;
    /**
     * 随机字符串
     */
    private String nonce_str;

    public TransferinfoReq(String sign, String partner_trade_no, String mch_id, String appid, String nonce_str) {
        this.sign = sign;
        this.partner_trade_no = partner_trade_no;
        this.mch_id = mch_id;
        this.appid = appid;
        this.nonce_str = nonce_str;
    }
}
